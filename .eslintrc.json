{
    "env": {
        "browser": true,
        "commonjs": true,
        "es2021": true
    },
    "extends": [
        "airbnb-base",
        "prettier"
    ],
    "parserOptions": {
        "ecmaVersion": "latest"
    },
    "rules": {
        "no-console": "off",
        "no-alert": "off",
        "prettier/prettier": [
          "error",
            {
            "trailingComma": "all",
            "semi": true,
            "printWidth": 80,
            "tabWidth": 2,
            "endOfLine": "auto"
            }
        ]
    }
}
